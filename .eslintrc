{
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "node": true,
      "jquery": true,
      "es6": true,
      "amd": true
    },
    "rules": {
      "no-console": 0,
      "no-undef": 2,
      "semi": [
        2,
        "always"
      ],
      "quotes": [
        2,
        "double"
      ],
      "eqeqeq": 2,
      "no-eq-null": 2,
      "block-scoped-var": 2,
      "no-unused-vars": 1
    }
}